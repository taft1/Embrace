{
  "name": "ui-website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@sentry/react": "^7.76.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "formik": "^2.4.5",
    "jsonwebtoken": "^9.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.17.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "yup": "^1.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run sentry:sourcemaps",
    "lint": "eslint",
    "test": "jest",
    "format": "prettier --write .",
    "eject": "react-scripts eject",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org embrace-dl --project javascript-react build && sentry-cli sourcemaps upload --org embrace-dl --project javascript-react build",
    "optimize:images": "imagemin"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^27.5.1",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "react-app-rewired": "^2.2.1",
    "react-testing-library": "^8.0.1"
  }
}
